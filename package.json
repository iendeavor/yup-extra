{
  "name": "yup-extra",
  "version": "1.0.0-alpha.0",
  "description": "yup-extra adds more useful methods that aren't included in the yup library",
  "repository": "git@github.com:iendeavor/yup-extra.git",
  "author": "Ernest <iendeavor.github.io@gmail.com>",
  "license": "MIT",
  "sideEffects": true,
  "keywords": [
    "yup"
  ],
  "files": [
    "lib",
    "es"
  ],
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "rollup -c rollup.config.cjs.js && rollup -c rollup.config.es.js",
    "release": "standard-version",
    "postrelease": "yarn build",
    "test": "yarn tsc --noEmit && jest",
    "lint": "prettier --check .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^11",
    "@commitlint/config-conventional": "^11",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/jest": "^26",
    "husky": "^6.0.0",
    "jest": "^26",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "standard-version": "^9.1.0",
    "ts-expect": "^1.1.0",
    "ts-jest": "^26",
    "ts-node": "^9.1.1",
    "typescript": "^4",
    "yup": "^0"
  },
  "peerDependencies": {
    "yup": "^0"
  }
}
